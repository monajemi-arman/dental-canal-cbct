{
  "predict": {
    "checkpoint": {
      "unet": "checkpoint.ckpt"
    }
  },
  "train": {
    "batch_size": 1,
    "max_epochs": 100,
    "accelerator": "gpu",
    "device": "cuda",
    "early_stop_callback": {
      "patience": 100
    }
  },
  "dataset": {
    "raw": "dataset",
    "split": {
      "train": 0.8,
      "val": 0.1,
      "test": 0.1
    },
    "transforms": [
      "normalize"
    ],
    "crop_target_size": [50, 50, 100],
    "dcm_directory_suffix": "_dcm",
    "dcm_file_pattern": "_(\\d+)\\.dcm$",
    "mask_suffix": ".nii",
    "compressed": 0,
    "all": "all",
    "train": "train",
    "val": "val",
    "test": "test",
    "images": "images",
    "masks": "masks"
  },
  "model": {
    "unet": {
      "spatial_dims": 3,
      "in_channels": 1,
      "out_channels": 1,
      "channels": [
        16,
        32,
        64,
        128,
        256
      ],
      "strides": [
        1,
        1,
        1,
        1
      ]
    }
  }
}